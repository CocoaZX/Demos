+ (NSDictionary *)formatThirdLoginParams:(NSString *)openid
                            access_token:(NSString *)access_token
                              expires_in:(NSString *)expires_in
                               plat_type:(NSInteger)plat_type
                                    idfa:(NSString *)idfa

{
    if (openid && access_token &&expires_in &&plat_type && idfa) {
        NSDictionary *allParams = @{@"openid":getSafeString(openid),
                                    @"access_token":getSafeString(access_token),
                                    @"expires_in":getSafeString(expires_in),
                                    @"plat_type":[NSNumber numberWithInteger:plat_type],
                                    @"idfa":getSafeString(idfa)};
        
        
        return [self getSignedDic:allParams];
    }
    return nil;
}

