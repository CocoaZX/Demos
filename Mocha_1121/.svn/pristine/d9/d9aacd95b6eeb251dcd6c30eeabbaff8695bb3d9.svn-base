+ (NSDictionary *)getSignedDic:(NSDictionary *)dic
{
    //  新签名
    NSMutableDictionary *allParams = dic.mutableCopy;
    [allParams setObject:@"1" forKey:@"clienttype"];
    
    NSTimeInterval time = [[NSDate date] timeIntervalSince1970];
    NSString *timeStamp = [NSString stringWithFormat:@"%.3f",time];
    
    [allParams setObject:timeStamp forKey:@"timestamp"];
    
    NSString *version = [[[NSBundle mainBundle] infoDictionary] objectForKey:(NSString*)kCFBundleVersionKey];
    //version = @"2.11";
    [allParams setObject:version?version:@"1.0" forKey:@"version"];
    
    NSString *tmpParam = [CommonUtils sortQuickPayParam:allParams];
    //    NSString *encodeParam = [URLHelper _encodeString:tmpParam];//  URLEncodedString  _encodeString
    NSString *encodeParam = tmpParam;//  URLEncodedString  _encodeString
    encodeParam = [encodeParam stringByReplacingOccurrencesOfString:@"%20" withString:@"+"];//注意
    encodeParam = [encodeParam stringByAppendingString:MOCHA_SECRET_KEY];
    NSString *md5Param = [CommonUtils md5ForString:encodeParam];
    [allParams setValue:md5Param forKey:@"sign"];//
    /*  老的签名
     NSMutableDictionary *allParams = dic.mutableCopy;
     [allParams setObject:@"1.0" forKey:@"version"];
     [allParams setObject:@"1" forKey:@"clienttype"];
     
     NSString *tmpParam = [CommonUtils sortQuickPayParam:allParams];
     NSString *encodeParam = [URLHelper _encodeString:tmpParam];//  URLEncodedString  _encodeString
     encodeParam = [encodeParam stringByReplacingOccurrencesOfString:@"%20" withString:@"+"];//注意
     encodeParam = [encodeParam stringByAppendingString:MOCHA_SECRET_KEY];
     NSString *md5Param = [CommonUtils md5ForString:encodeParam];
     [allParams setValue:md5Param forKey:@"sign"];//
     */
    return allParams.copy;
    
}

+ (NSDictionary *)formatVerifyParams:(NSString *)phone withType:(NSString *)type
