+ (NSDictionary *)formatLoginParams:(NSString *)phone
                             verify:(NSString *)verify
                           password:(NSString *)password
                          loginType:(NSInteger)loginType
{
    CLLocationDegrees lat = Latitude;
    CLLocationDegrees lng = Longitude;
    NSMutableDictionary *allParams = [NSMutableDictionary dictionaryWithDictionary:@{NetworkKeyMobile:getSafeString(phone),NetworkKeyPass:getSafeString(password)}];
    if ([verify length] > 0) {
        [allParams setValue:verify forKey:NetworkKeyCode];
        [allParams setValue:[NSNumber numberWithInteger:loginType] forKey:NetworkKeyLoginType];
    }
    [allParams setValue:[NSNumber numberWithDouble:lat] forKey:@"latitude"];
    [allParams setValue:[NSNumber numberWithDouble:lng]  forKey:@"longitude"];
    
    return [self getSignedDic:allParams];
}


+ (NSDictionary *)formatSignUpParams:(NSString *)phone
                              verify:(NSString *)verify
                            password:(NSString *)password
                                type:(NSString *)type
                       inviteMokaNum:(NSString *)mokaNum
{
    CLLocationDegrees lat = Latitude;
    CLLocationDegrees lng = Longitude;
    //    NSString *idfv = [[[UIDevice currentDevice] identifierForVendor] UUIDString];
    NSString *idfa = [[[ASIdentifierManager sharedManager] advertisingIdentifier] UUIDString];
    
    NSDictionary *allParams = @{@"idfa":getSafeString(idfa),NetworkKeyMobile:getSafeString(phone), NetworkKeyCode:getSafeString(verify), NetworkKeyPass:getSafeString(password),@"type":getSafeString(type),@"latitude":[NSNumber numberWithDouble:lat],@"longitude":[NSNumber numberWithDouble:lng],@"invite":getSafeString(mokaNum)};
    
    return [self getSignedDic:allParams];
}
+ (NSDictionary *)formatNewSignUpParams:(NSString *)phone
                                 verify:(NSString *)verify
                               password:(NSString *)password
                                   type:(NSString *)type
                          inviteMokaNum:(NSString *)mokaNum
{
    CLLocationDegrees lat = Latitude;
    CLLocationDegrees lng = Longitude;
    //    NSString *idfv = [[[UIDevice currentDevice] identifierForVendor] UUIDString];
    NSString *idfa = [[[ASIdentifierManager sharedManager] advertisingIdentifier] UUIDString];
    NSDictionary *allParams = nil;
    if (mokaNum.length>0) {
        allParams = @{@"idfa":getSafeString(idfa),NetworkKeyMobile:getSafeString(phone), NetworkKeyCode:getSafeString(verify), NetworkKeyPass:getSafeString(password),@"latitude":[NSNumber numberWithDouble:lat],@"longitude":[NSNumber numberWithDouble:lng],@"invite":getSafeString(mokaNum)};
        
    }else
    {
        allParams = @{@"idfa":getSafeString(idfa),NetworkKeyMobile:getSafeString(phone), NetworkKeyCode:getSafeString(verify), NetworkKeyPass:getSafeString(password),@"latitude":[NSNumber numberWithDouble:lat],@"longitude":[NSNumber numberWithDouble:lng]};
        
    }
    
    return [self getSignedDic:allParams];
}
